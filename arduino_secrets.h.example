// arduino_secrets.h.example
// Copy this file to arduino_secrets.h and fill in your actual credentials
// NEVER commit arduino_secrets.h to git - it's in .gitignore for your protection

#ifndef ARDUINO_SECRETS_H
#define ARDUINO_SECRETS_H

// WiFi Network Credentials
// Note: Arduino R4 WiFi uses Network Configurator with BLE/Serial provisioning
// These secrets may be set via BLE or Serial instead of hardcoding
// If using provisioning, you can leave these empty
#define SECRET_WIFI_SSID ""
#define SECRET_WIFI_PASS ""

// Arduino IoT Cloud Credentials
// These are automatically provisioned when you set up your device in Arduino IoT Cloud
// You typically don't need to set these manually
#define SECRET_DEVICE_KEY ""

// Optional: Webhook Configuration
// If using custom webhook logging (like webhook.site)
#define WEBHOOK_URL "https://webhook.site/YOUR-UNIQUE-ID"

#endif
